{
  "name": "booking-api",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "babel-version": "babel --version",
    "start": "nodemon ./index.js --exec babel-node",
    "populate": "NODE_ENV=dev  babel-node ./populate.data.js  ",
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "NODE_ENV=testing ./node_modules/.bin/jest --forceExit --silent  --detectOpenHandles --testTimeout=5000",
    "test-routes": "npm test -t router",
    "test-models": "npm test -t model",
    "test-controllers": "npm test -t controllers",
    "test-auth": "npm test -t auth"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.6",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validator": "^6.2.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.1",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/regenerator-runtime/runtime"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ]
  }
}
